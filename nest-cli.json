{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "compilerOptions": {
    "assets": [
      {
        "include": "modules/app/mail/templates/*.hbs",
        "outDir": "dist"
      }
    ],
    "watchAssets": true,
    "deleteOutDir": true
  },
  "projects": {
    "main": {
      "type": "application",
      "entryFile": "main"
    },
    "stat": {
      "type": "application",
      "entryFile": "mainStat"
    }
  }
}