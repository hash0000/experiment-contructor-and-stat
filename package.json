{
  "name": "experiment-constructor-and-stat",
  "version": "0.0.1",
  "author": "Nikita Mosin",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "start": "nest start",
    "start-stat": "nest start stat",
    "start:prod": "cross-env NODE_ENV=production node dist/main",
    "start-stat:prod": "cross-env NODE_ENV=production node dist/mainStat",
    "start:dev": "nest start main --watch",
    "start-stat:dev": "nest start stat --watch",
    "start:debug": "nest start --debug --watch",
    "typeorm": "ts-node ./node_modules/typeorm/cli",
    "db:run-migrations": "cross-env MIGRATION_PATH=migrations cross-env NODE_ENV=development npx typeorm migration:run -d ./dist/common/configs/typeorm.config.js",
    "db:run-seeds": "cross-env MIGRATION_PATH=seeds cross-env NODE_ENV=development npx typeorm migration:run -d ./dist/common/configs/typeorm.config.js",
    "db:generate-migration": "npx typeorm -d ./dist/common/configs/typeorm.config.js migration:generate src/modules/app/database/migrations/test",
    "db:create-seed": "npx typeorm migration:create src/modules/app/database/seeds/test",
    "db:create-migration": "npx typeorm -d ./dist/common/configs/typeorm.config.js migration:create src/modules/app/database/migrations/test",
    "db:run-migrations:prod": "cross-env MIGRATION_PATH=migrations cross-env NODE_ENV=production npx typeorm migration:run -d ./dist/common/configs/typeorm.config.js",
    "db:run-seeds:prod": "cross-env MIGRATION_PATH=seeds cross-env NODE_ENV=production npx typeorm migration:run -d ./dist/common/configs/typeorm.config.js"
  },
